<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.noob.controller.MainSceneController">
   <children>
      <TextField fx:id="pathTextField" layoutX="62.0" onAction="#searchDirectory" prefHeight="30.0" prefWidth="540.0" />
      <Button fx:id="upButton" layoutY="-1.0" mnemonicParsing="false" onAction="#upDirectory" prefHeight="30.0" prefWidth="60.0" text="Up" />
      <ListView fx:id="fileListView" layoutY="30.0" onMouseClicked="#selectItem" prefHeight="370.0" prefWidth="400.0" />
   </children>
</AnchorPane>
